<% core.app.wiki.pieces.header_wikipage(); %>

<ul id="wiki_history" class="content">
<%
    wikiPageHistory = wikiPage.getWikiPageHistories();
    while (wikiPageHistory.hasNext()) {
        var historicalPage = wikiPageHistory.next();
%>
        <li><a href="?version=<%=historicalPage._id%>"><%= historicalPage.ts %></a> <a href="?version=<%= historicalPage._id %>&diff=true">diff</a></li>
    <% } %>
</ul>

<% core.app.wiki.pieces.footer_wikipage(); %>

